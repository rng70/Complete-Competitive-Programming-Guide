@charset "utf-8";

.css-sprite-quiz { background-image: url("/site_media/version-V161cb53b632/build/sprites/css-sprite-quiz.png"); }

@media (-webkit-min-device-pixel-ratio: 1.5), (min-resolution: 1.5dppx) {
  .css-sprite-quiz { background-image: url("/site_media/version-V161cb53b632/build/sprites/css-sprite-quiz-x2.png"); background-size: 290px 278px; }
}

.css-sprite-nux { background-image: url("/site_media/version-V8516df3c312/build/sprites/css-sprite-nux.png"); }

@media (-webkit-min-device-pixel-ratio: 1.5), (min-resolution: 1.5dppx) {
  .css-sprite-nux { background-image: url("/site_media/version-V8516df3c312/build/sprites/css-sprite-nux-x2.png"); background-size: 404px 250px; }
}

.nux-anim-earth { display: block; width: 160px; height: 160px; margin: 12px auto 0px; }

.nux-anim-earth svg { background: rgb(2, 119, 189); border-radius: 50%; }

.nux-anim-earth .land { fill: rgb(90, 206, 66); }

.nux-anim-game { width: 404px; height: 63px; margin: 56px auto 0px; }

.nux-anim-game .css-sprite-nux { position: absolute; background-position: -259px -128px; width: 19px; height: 17px; z-index: 1; }

.nux-anim-game .die { bottom: 29px; left: 29px; animation: die 3s cubic-bezier(0.64, 0.57, 0.67, 1.53) 0ms infinite normal forwards running; }

.nux-anim-game .pyramid { bottom: 28px; left: 48px; width: 58px; height: 56px; background-position: 0px -194px; animation: pyramid 4s cubic-bezier(0.64, 0.57, 0.67, 1.53) -1.5s infinite normal forwards running; }

@keyframes pyramid { 
  0% { transform: translate(0px); }
  50% { transform: translateY(-8px); }
  100% { transform: translate(0px); }
}

.nux-anim-game .dodecahedron { bottom: 16px; left: 99px; width: 50px; height: 48px; background-position: -60px -194px; animation: dodecahedron 5s cubic-bezier(0.64, 0.57, 0.67, 1.53) -2.5s infinite normal forwards running; }

@keyframes dodecahedron { 
  0% { transform: translate(0px); }
  50% { transform: translateY(-12px); }
  100% { transform: translate(0px); }
}

.nux-anim-game .marbles span { position: absolute; bottom: 19px; left: 192px; background-position: -395px -65px; width: 9px; height: 9px; animation: die 3s cubic-bezier(0.64, 0.57, 0.67, 1.53) -2s infinite normal forwards running; }

.nux-anim-game .marbles span:first-child { bottom: 17px; left: 175px; animation: die 2.5s cubic-bezier(0.64, 0.57, 0.67, 1.53) -1.5s infinite normal forwards running; }

.nux-anim-game .marbles span:last-child { bottom: 14px; left: 208px; animation: die 3.5s cubic-bezier(0.64, 0.57, 0.67, 1.53) -2.5s infinite normal forwards running; }

.nux-anim-game .bag { bottom: 6px; left: 224px; width: 75px; height: 39px; background-position: -264.5px -65px; animation: die 4s cubic-bezier(0.64, 0.57, 0.67, 1.53) -1s infinite normal forwards running; }

.nux-anim-game .shape { bottom: 23px; left: 308px; width: 79px; height: 44px; background-position: -88px -128px; animation: die 6s cubic-bezier(0.64, 0.57, 0.67, 1.53) -3s infinite normal forwards running; }

@keyframes die { 
  0% { transform: translate(0px); }
  50% { transform: translateY(-18px); }
  100% { transform: translate(0px); }
}

.nux-anim-game .table { width: 404px; height: 63px; background-position: 0px 0px; z-index: 0; }

.nux-anim-paintbrush { display: block; width: 302px; margin: 64px auto 0px; }

.nux-anim-paintbrush .css-sprite-nux { background-position: -88px -108px; background-repeat: no-repeat; overflow: hidden; display: block; width: 302px; height: 18px; }

.nux-anim-paintbrush .drip { position: relative; display: block; float: right; width: 100px; height: 80px; margin: -7px -44px 0px 0px; }

.nux-anim-paintbrush .drip::before { content: ""; position: absolute; top: 0px; left: 47.5px; width: 5px; height: 5px; border-radius: 50%; background-color: rgb(2, 119, 189); opacity: 0; animation: paintbrushdrip 4s ease 1s infinite normal none running; }

.nux-anim-paintbrush .drip::after { content: ""; position: absolute; bottom: 0px; left: 0px; width: 0px; height: 0px; border: 1px solid rgb(2, 119, 189); border-radius: 50%; opacity: 0; box-sizing: border-box; animation: paintbrushsplash 4s ease 1s infinite normal none running; }

@keyframes paintbrushdrip { 
  10% { top: 0px; opacity: 1; animation-timing-function: cubic-bezier(0.24, 0, 0.76, 0.14); }
  30% { opacity: 1; top: 70px; animation-timing-function: ease-out; width: 5px; height: 5px; left: 47.5px; }
  30.001% { opacity: 0; top: 80px; }
}

@keyframes paintbrushsplash { 
  0% { opacity: 0; }
  30% { bottom: 10px; left: 50px; opacity: 0; width: 0px; height: 0px; }
  30.001% { opacity: 1; }
  40% { bottom: 0px; left: 0px; opacity: 0; width: 100px; height: 20px; }
}

.nux-anim-flame { display: block; position: absolute; bottom: 0px; left: 50%; width: 74px; height: 164px; margin-left: -37px; }

.nux-anim-flame .stick { position: absolute; bottom: 0px; left: 50%; width: 20px; height: 90px; border-left: 6px solid rgb(170, 121, 86); margin-left: -10px; background: rgb(219, 169, 127); }

.nux-anim-flame .fire { display: block; position: absolute; top: 0px; width: 74px; height: 74px; }

.nux-anim-flame .fire .flame { position: absolute; bottom: 0px; left: 0px; opacity: 0.9; width: 100%; height: 100%; border-radius: 70% 0px 70% 70%; background: rgb(255, 225, 67); transform: rotate(-45deg) skew(-10deg, -10deg); animation: flame1 0.3s ease 0s infinite normal forwards running; z-index: 1; }

.nux-anim-flame .fire .flame:nth-of-type(2n) { animation-name: flame2; animation-delay: 0s; animation-duration: 0.4s; }

.nux-anim-flame .fire .flame:nth-of-type(2) { height: 80%; width: 80%; left: 10%; background: rgb(252, 160, 74); z-index: 2; }

.nux-anim-flame .fire .flame:nth-of-type(3) { height: 50%; width: 50%; left: 25%; background: rgb(255, 102, 77); z-index: 3; }

.nux-anim-flame .spark { position: absolute; bottom: 100px; left: 15%; width: 3px; height: 3px; background-color: rgba(252, 160, 74, 0.992); animation: spark1 6s linear 0.4s infinite normal none running; }

